{
  "distance_influence": 1,
  "priority": [
    { "if": "hgv == NO",                                 "multiply_by": "0" },
    { "if": "road_access == PRIVATE",                    "multiply_by": "0" },
    { "if": "car_access == false || max_width < 2.5 || max_height < 2.7 || max_weight < 3", "multiply_by": "0" }
  ],
  "speed": [
    { "if": "true",                                      "limit_to": "car_average_speed * 0.9" },
    { "if": "max_height < 4.5",                          "multiply_by": "0.5" },
    { "if": "max_weight < 40",                           "multiply_by": "0.5" }
  ]
}
